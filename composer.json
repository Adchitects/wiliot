{
    "require": {
        "algolia/algoliasearch-client-php": "^3.3",
        "born05/craft-assetusage": "2.3.1",
        "craftcms/cms": "3.8.10.2",
        "craftcms/redactor": "2.10.12",
        "dolphiq/redirect": "1.1.1",
        "dolphiq/sitemap": "1.3.2",
        "ether/seo": "3.7.4",
        "gudehus/searchexcerpt": "1.3.1",
        "mustasj/expanded-search": "1.3.7",
        "sebastianlenz/linkfield": "1.0.25",
        "utakka/redactor-anchors": "1.4.1",
        "verbb/field-manager": "2.2.5",
        "verbb/image-resizer": "2.2.0",
        "verbb/smith": "1.2.3",
        "verbb/super-table": "2.7.5.1",
        "verbb/tablemaker": "3.0.4",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3.33"
        },
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
