{% extends "index.twig" %}
{% set metaTitle = entry.metaTitle %}
{% set metaDescription = entry.metaDescription %}
{% block head %} {{ parent() }} {% endblock %}
{% block title %}
	{{ metaTitle ? metaTitle : entry.title ~ ' | ' ~ siteName  }}
{% endblock %}
{% block description %}
	<meta name="description" content="{{ metaDescription ? metaDescription : globalMetaTags.metaDescription }}">
{% endblock %}
{% block og %}
	<meta property="og:title" content="{{ metaTitle ? metaTitle : globalMetaTags.metaTitle }}">
	<meta property="og:description" content="{{ metaDescription ? metaDescription : globalMetaTags.metaDescription }}">
	<meta property="og:type" content="website">
{% endblock %}
{% block main %}
	{% set sectionId = '01' %}
	{{ include('/sections/product-hero.twig') }}
	{% set sectionId = '02' %}
	{{ include('/sections/fifty-fifty.twig') }}
	{% set sectionId = '03' %}
	{{ include('/sections/product-features-v2.twig') }}
	{% set sectionId = '04' %}
	{{ include('/sections/comparison-table.twig') }}
	{% set sectionId = '05' %}
	{{ include('/sections/product-other.twig') }}
	{% set sectionId = '06' %}
	{{ include('/sections/product-other-v2.twig') }}
	{% set sectionId = '07' %}
	{{ include('/sections/tabs.twig') }}
	{% set sectionId = '08' %}
	{{ include('/sections/outro.twig') }}
	{% set sectionId = '' %}
{% endblock %}
